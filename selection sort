#include <iostream>
using namespace std;

int main() {
    int data[5] = {64, 25, 12, 22, 11};
    int n = 5;

    for (int i = 0; i < n-1; i++) {
        int minIndex = i;
        for (int j = i+1; j < n; j++) {
            if (data[j] < data[minIndex]) {
                minIndex = j;
            }
        }
        // tukar elemen
        int temp = data[i];
        data[i] = data[minIndex];
        data[minIndex] = temp;
    }

    for (int i = 0; i < n; i++) {
        cout << data[i] << " ";
    }
}
